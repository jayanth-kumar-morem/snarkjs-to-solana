{
  "name": "snarkjs-to-solana",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jayanth Kumar <jayanthkumar1903@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "examples/**/*",
    "proof_utils/pkg/*",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "ffjavascript": "^0.3.1",
    "proofUtils": "file:./proof_utils/pkg"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prePublishOnly": "npm run build && npm test",
    "prepare": "npm run build",
    "buid:proofUtils:bundler": "cd proof_utils && wasm-pack build --target bundler && cd -",
    "build:proofUtils:nodejs": "cd proof_utils && wasm-pack build --target nodejs && cd -"
  },

}
